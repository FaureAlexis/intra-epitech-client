{
  "name": "intra-epitech-client",
  "version": "1.0.6",
  "description": "",
  "main": "build/index",
  "types": "build/index.d.ts",
  "files": [
    "build",
    "package.json",
    "README.md"
  ],
  "scripts": {
    "dev": "nodemon",
    "test": "jest",
    "test:dev": "jest --detectOpenHandles --watch",
    "build": "rm -rf build && tsc",
    "lint": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0 --fix",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint",
    "version": "npm run lint && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/FaureAlexis/intra-epitech-client.git"
  },
  "keywords": [
    "epitech",
    "intra",
    "client",
    "api",
    "typescript",
    "intra.epitech.eu",
    "wrapper"
  ],
  "author": "alexis.faure@epitech.eu",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/FaureAlexis/intra-epitech-client/issues"
  },
  "homepage": "https://github.com/FaureAlexis/intra-epitech-client#readme",
  "devDependencies": {
    "@types/jest": "^29.5.1",
    "@types/node": "^18.15.11",
    "@typescript-eslint/eslint-plugin": "^5.59.5",
    "@typescript-eslint/parser": "^5.59.5",
    "eslint": "^8.40.0",
    "jest": "^29.5.0",
    "nodemon": "^2.0.22",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.4"
  },
  "dependencies": {
    "axios": "^1.3.5",
    "dotenv": "^16.0.3"
  }
}
